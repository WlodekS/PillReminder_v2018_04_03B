<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView_Schedule_Hour"
        android:layout_width="32dp"
        android:layout_height="46dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/fjalla_one"
        android:gravity="right"
        android:text="TextView"
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_Schedule_Minute"
        android:layout_width="28dp"
        android:layout_height="46dp"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/fjalla_one"
        android:gravity="left"
        android:text="TextView"
        android:textSize="25sp"
        app:layout_constraintStart_toEndOf="@+id/textView_Schedule_Colon"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_Schedule_Colon"
        android:layout_width="9dp"
        android:layout_height="46dp"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/fjalla_one"
        android:text="TextView"
        android:textSize="25sp"
        app:layout_constraintStart_toEndOf="@+id/textView_Schedule_Hour"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_Schedule_MediceName"
        android:layout_width="124dp"
        android:layout_height="37dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/fjalla_one"
        android:text="TextView"
        android:textColor="@color/colorListMedineName"
        android:textSize="25sp"
        app:layout_constraintStart_toEndOf="@+id/textView_Schedule_Minute"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_Schedule_Dose"
        android:layout_width="59dp"
        android:layout_height="24dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/fjalla_one"
        android:text="TextView"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/textView_Schedule_Minute"
        app:layout_constraintTop_toBottomOf="@+id/textView_Schedule_MediceName" />

    <TextView
        android:id="@+id/textView_Form"
        android:layout_width="66dp"
        android:layout_height="37dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/fjalla_one"
        android:text="TextView"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/textView_Schedule_MediceName"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_Dose_Option"
        android:layout_width="68dp"
        android:layout_height="24dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/fjalla_one"
        android:text="TextView"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/textView_Schedule_Dose"
        app:layout_constraintTop_toBottomOf="@+id/textView_Schedule_MediceName" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="34dp"
        android:layout_height="31dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="24dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        android:contentDescription="Edit"
        android:onClick="scheduleEdit"
        app:layout_constraintBottom_toBottomOf="@+id/textView_Dose_Option"
        app:layout_constraintEnd_toStartOf="@+id/imageView10"
        app:layout_constraintStart_toEndOf="@+id/textView_Form"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/icona_zielone_ok" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="32dp"
        android:layout_height="31dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="32dp"
        android:contentDescription="Delete"
        android:onClick="ScheduleDelete"
        app:layout_constraintBottom_toBottomOf="@+id/textView_Dose_Option"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/icona_czerwony_x" />

    <TextView
        android:id="@+id/textView_current_ID"
        android:layout_width="8dp"
        android:layout_height="8dp"
        android:text="TextView"
        android:visibility="invisible"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <TextView
        android:id="@+id/textView_Sun"
        android:layout_width="30dp"
        android:layout_height="23dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/fjalla_one"
        android:text="Sun"
        android:textSize="15sp"
        app:layout_constraintStart_toEndOf="@+id/textView_Schedule_Minute"
        app:layout_constraintTop_toBottomOf="@+id/textView_Schedule_Dose" />

    <TextView
        android:id="@+id/textView_Mon"
        android:layout_width="35dp"
        android:layout_height="23dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/fjalla_one"
        android:text="Mon"
        android:textSize="15sp"
        app:layout_constraintStart_toEndOf="@+id/textView_Sun"
        app:layout_constraintTop_toBottomOf="@+id/textView_Schedule_Dose" />

    <TextView
        android:id="@+id/textView_Tue"
        android:layout_width="30dp"
        android:layout_height="23dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/fjalla_one"
        android:text="Tue"
        android:textSize="15sp"
        app:layout_constraintStart_toEndOf="@+id/textView_Mon"
        app:layout_constraintTop_toBottomOf="@+id/textView_Schedule_Dose" />

    <TextView
        android:id="@+id/textView_Wed"
        android:layout_width="35dp"
        android:layout_height="23dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/fjalla_one"
        android:text="Wed"
        android:textSize="15sp"
        app:layout_constraintStart_toEndOf="@+id/textView_Tue"
        app:layout_constraintTop_toBottomOf="@+id/textView_Schedule_Dose" />

    <TextView
        android:id="@+id/textView_Thu"
        android:layout_width="30dp"
        android:layout_height="23dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/fjalla_one"
        android:text="Thu"
        android:textSize="15sp"
        app:layout_constraintStart_toEndOf="@+id/textView_Wed"
        app:layout_constraintTop_toBottomOf="@+id/textView_Schedule_Dose" />

    <TextView
        android:id="@+id/textView_Fri"
        android:layout_width="26dp"
        android:layout_height="23dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/fjalla_one"
        android:text="Fri"
        android:textSize="15sp"
        app:layout_constraintStart_toEndOf="@+id/textView_Thu"
        app:layout_constraintTop_toBottomOf="@+id/textView_Schedule_Dose" />

    <TextView
        android:id="@+id/textView_Sat"
        android:layout_width="27dp"
        android:layout_height="23dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/fjalla_one"
        android:text="Sat"
        android:textSize="15sp"
        app:layout_constraintStart_toEndOf="@+id/textView_Fri"
        app:layout_constraintTop_toBottomOf="@+id/textView_Schedule_Dose" />


</android.support.constraint.ConstraintLayout>